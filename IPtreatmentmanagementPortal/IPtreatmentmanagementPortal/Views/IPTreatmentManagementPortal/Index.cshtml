@{
    ViewData["Title"] = "Dashboard";
}


<div id="wrapper">

    
    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">

        <!-- Main Content -->
        <div id="content">
          
            <!-- Begin Page Content -->
            <div class="container-fluid">

                <!-- Page Heading -->
                <div class="d-sm-flex align-items-center justify-content-between mb-4">
                    <h1>GetTreatmentPlans</h1>
                    
                    
                </div>

                <!-- Content Row -->
                <div class="row">

                    <!-- View Treatment Packages Card -->
                    <div class="col-xl-3 col-md-6 mb-4">
                        <div class="card border-left-primary shadow h-100 py-2">
                            <div class="card-body">
                                <a class="btn btn" asp-area="" asp-controller="IPtreatmentmanagementPortal" asp-action="GetTreatmentPackages">View Treatment Packages</a>
                            </div>
                        </div>
                    </div>

                    <!-- View Patients Card  -->
                    <div class="col-xl-3 col-md-6 mb-4">
                        <div class="card border-left-success shadow h-100 py-2">
                            <div class="card-body">
                                <a class="btn btn" asp-area="" asp-controller="IPtreatmentmanagementPortal" asp-action="GetPatientDetails">View Patients</a>
                            </div>
                        </div>
                    </div>

                    <!-- View Specialists Card -->
                    <div class="col-xl-3 col-md-6 mb-4">
                        <div class="card border-left-info shadow h-100 py-2">
                            <div class="card-body">
                                <a class="btn btn" asp-area="" asp-controller="IPtreatmentmanagementPortal" asp-action="GetSpecialistDetails">View Specialists</a>
                            </div>
                        </div>
                    </div>

                    <!-- View Insurance Providers Card -->
                    <div class="col-xl-3 col-md-6 mb-4">
                        <div class="card border-left-warning shadow h-100 py-2">
                            <div class="card-body">
                                <a class="btn btn" asp-area="" asp-controller="IPtreatmentmanagementPortal" asp-action="GetInsuranceDetails">View Insurance Providers</a>
                            </div>
                        </div>
                    </div>

                    <!-- View All Treatment Plans Card -->
                    <div class="col-xl-3 col-md-6 mb-4">
                        <div class="card border-left-warning shadow h-100 py-2">
                            <div class="card-body">
                                <a class="btn btn" asp-area="" asp-controller="IPtreatmentmanagementPortal" asp-action="GetTreatmentPlans">View All Treatment Plans</a>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
            <!-- /.container-fluid -->

        </div>
        <!-- End of Main Content -->
        

    </div>
    <!-- End of Content Wrapper -->

</div>
<!-- End of Page Wrapper -->
